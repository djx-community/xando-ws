{
  "name": "xando",
  "version": "1.0.0",
  "description": "tic tac toe App",
  "main": "app.js",
  "repository": "https://github.com/dev-jsx/toenail-api",
  "author": "midlajc | ali",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "prod": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "yarn install --dev && yarn install && tsc && tsc-alias",
    "prisma:init": "dotenv -e .env -- prisma init",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
    "prisma:generate": "dotenv -e .env -- prisma generate",
    "prisma:studio": "dotenv -e .env -- prisma studio"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@socket.io/admin-ui": "^0.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "socket.io": "^4.5.2",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/uniqid": "^5.3.2",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.3"
  }
}
